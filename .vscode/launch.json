    "version": "0.2.0",
    "configurations": [{
            "type": "node",
            "request": "launch",
            "name": "Launch NPM test",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "test"
            ],
            "port": 9229,
            "protocol": "legacy"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\lib\\winston-mongodb"
        }, {
            "type": "node",
            "request": "launch",
            "name": "Launch Tests",
            "program": "${workspaceFolder}/node_modules/bin/vows",
            "args": [
                "test/*-test.js",
                "--spec"
            ]
        }
    ]
}